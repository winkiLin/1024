.userinfo {
  text-align: center;
  height: 340rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size:100% 100%;
}

.userinfo button{
  display: inline-block;
  width: 100rpx;
  backgournd: #6b85f5;
  filter:alpha(opacity=50);   
  -moz-opacity:0.5;
  opacity:0.5;
  color: #000;
  border-radius: 10rpx;
  /* margin-top: 80rpx; */
}
/*未登录*/
.login-no{
    margin-top: 80rpx;
    text-align: center;
    display: inline-block;
}
.login-no image{
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    background: white;
    border: #6b85f5 solid 4rpx;
}
.login-no view{
    margin-top: 20rpx;
    width: 100%;
    text-align: center;
}
.login-no text{
    display: inline-block;
    width: 200rpx;
    height: 50rpx;
    line-height: 46rpx;
    border-radius: 50rpx;
    color: white;
    text-align: center;
    border: white solid 2rpx;
    font-size: 13px;
}
.login-btn{
  width: 100rpx;
  
}
.login-yes{
  width: 100%;
  text-align: center;
  margin-top: 80rpx;
  display: inline-block;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  display: block;
  color: #fff;
  margin-top: 20rpx;
}

.items{
  width: 96%;
  margin: auto;
}

.item{
  position: relative;
  height: 110rpx;
  border-bottom: #dedede solid 2rpx;
}

.item .left{
  display: inline-block;
  position: absolute;
  left: 0;
  color: #484848;
  font-size: 16px;
  top: 27rpx;
}

.item image{
  width: 30rpx;
  height: 30rpx;
  top: 36rpx !important;
}
.item .right{
  display: inline-block;
  position: absolute;
  right: 0;
  top: 31rpx;
  font-size: 14px;
  color: #a5a5a5;
}
